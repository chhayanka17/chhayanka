#include <stdio.h>
#include <math.h>
int main(){
int a, b, c, d, e, f;
printf("Enter any number\t");
scanf ("%d", &a);
printf("Enter the number of digits\t");
scanf ("%d", &f);
e = a;
d = a%10;
a - a/10;
c = a%10;
a = a/10;
b = a%10;
a = a/10;{
if(pow(d,f)+pow(c,f)+pow(b,f) = e)
printf("%d is an Armstrong number", e);
else
printf("%d is not an Armstrong number", e);
return 0;
}
