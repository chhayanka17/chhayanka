#include <stdio.h>

int main() {
    int i, j, k = 5;
    
    for(i = 1; i <= 5; i++)
    {  k = 5;
        for(j = 1; j <= i; j++) {
            printf("%d\t", k);
            k--;
        }
        printf("\n"); // move to the next line after each row
    }

    return 0;
}





5	
5	4	
5	4	3	
5	4	3	2	
5	4	3	2	1	
output
