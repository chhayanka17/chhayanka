#include <stdio.h>
#include <math.h>

int main() {
    int a, i ,n, b, sum;
    sum = 0;
    printf("Enter any number to check if is an armstrong number or not:\t");
    scanf("%d",&a);
      b = a;
    for(i = a; i!=0; i = i/10){
    n = i%10;
    sum = sum + pow(n, 3);
    }
    if(b == sum)
    printf("%d is an armstrong number", b);
    else
    printf("%d is not an armstrong number", b);

    return 0;
}
