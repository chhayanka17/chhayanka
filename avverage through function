#include <stdio.h>
int average(int,int,int);
int main() {
    int a, b, c;
    printf("Enter three marks:\n");
    scanf("%d %d %d", &a, &b, &c);
    average(a, b, c);
    printf("end");
    return 0;
}
int average(int a, int b, int c) { 
    int d ;
    d = (a + b + c)/3 ;
    printf("average = %d\n", d);
    return 0;
}
