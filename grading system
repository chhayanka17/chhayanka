#include <stdio.h>

int main() {
    int x;
    printf("Enter Marks (0-100): ");
    scanf("%d", &x);
    if(x>=30 && x<70){
        printf("Your grade is B");
    }
    else if(x>=70 && x<=90){
        printf("Your grade is A");
    }
    else if(x<30 && x>=0){
        printf("Your grade is C");
    }
    else if(x>90 && x<=100){
        printf("Your grade is A+");
    }
    else {
        printf("Invalid Marks");
    }
}
