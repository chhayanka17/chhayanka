#include <stdio.h>

int main() {
   int c;
   printf("Enter your choice:\t");
   scanf("%d", &c);
   switch(c)
   {
       case 1: 
            printf("Monday\n");
            break;
       
       case 2: 
            printf("Tuesday\n");
            break;
       
       case 3: 
            printf("wednesday\n");
            break;
       
       case 4: 
            printf("Thursday\n");
            break;
       case 5: 
            
            printf("Friday\n");
            break;
      case 6: 
            printf("Saturday\n");
            break;
      case 7: 
            printf("Sunday\n");
            break;
   
       default:
            printf("This is the wrong command\n");
   } // end switch
   printf("end");
}
