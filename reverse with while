#include <stdio.h>

int main() {
    int a, b,cnt=0,n, rev=0,t;
    printf("Enter any number:\t");
    scanf("%d", &a);
    b = a;
    t=100;
    while(a!=0){
        n = a%10;
        cnt++;
        a = a/10;
        rev = rev+(n*t) ;
        t=t/10;
        
    }
    printf("The no. of digits in %d is %d\n", b,cnt );
    printf("The reverse of %d will be %d\n", b, rev);
    return 0;
}
